# Find the TH includes and library
#
# ATEN_INCLUDE_DIR -- where to find the includes
# ATEN_LIBRARIES -- list of libraries to link against
# ATEN_FOUND -- set to 1 if found

SET(ATEN_FOUND 1)
SET(ATEN_INCLUDE_DIR "/usr/local/lib/python3.5/dist-packages/torch/lib/include")
SET(ATEN_LIBRARIES "/usr/local/lib/python3.5/dist-packages/torch/lib/libATen.so.1")
